msgid "about-message"
msgstr "Ceci est une application web Spynl. Vous pouvez obtenir plus d'informations en visitant about/endpoints, about/ini, about/versions, about/build et about/environment."

msgid "validation-error"
msgstr "Désolé, il y a eu un problème avec votre demande. Si le problème persiste, veuillez contacter notre équipe d'assistance. Nous nous excusons pour le dérangement."

msgid "no-endpoint-for-path"
msgstr "Aucun endpoint n'a été trouvé pour le path '${path}'."

msgid "permission-denial"
msgstr "La permission de '${permission}' ${context} a été refusée."

msgid "internal-server-error"
msgstr "Une erreur interne du serveur est survenue."

msgid "bad-origin"
msgstr "Les requêtes à l'API Spynl ne sont pas autorisées à partir de l'origine '${origin}'."

msgid "missing-parameter"
msgstr "Paramètre requis manquant : ${param}"

msgid "illegal-parameter"
msgstr "Paramètre illégal : ${param}"

msgid "bad-validation-instructions"
msgstr "La description des validations pour cet endpoint ne peut pas être utilisée : ${error}"

msgid "invalid-response"
msgstr "Spynl n'a pas pu générer une réponse valide : ${error}"

msgid "email-tmpl-not-found"
msgstr "Le modèle d'e-mail <${template}> n'a pas été trouvé."

msgid "email-recipient-not-given"
msgstr "Vous n'avez pas indiqué le destinataire de l'e-mail."

msgid "undetermined-content-type-exception"
msgstr "La requête contient un corps mais le type de contenu ne peut pas être déterminé."

msgid "unsupported-content-type-exception"
msgstr "Type de contenu non pris en charge : \"${type}\""

msgid "deserialization-unsupported-exception"
msgstr "La désérialisation pour le contenu de type \"${type}\" n'est pas prise en charge."

msgid "serialization-unsupported-exception"
msgstr "La sérialisation pour le contenu de type : \"${type}\" n'est pas prise en charge."

msgid "malformed-request-exception-type"
msgstr "Requête \"${type}\" mal formée : ${request}."

msgid "malformed-request-exception"
msgstr "Requête malformée : ${type}"

msgid "date-decode-value-error"
msgstr "La valeur '${value}' de la clé '${key}' ne semble pas être une date valide qui est conforme au format ${format}."

msgid "unexpected-end-of-input"
msgstr "Fin d'entrée inattendue."

msgid "parse-value-exception-as-class"
msgstr "\"${value}\" ne peut pas être analysé comme ${class}"

msgid "parse-value-exception-not-class"
msgstr "${class} n'est pas une classe. Impossible d'analyser la valeur \"${value}\"."

msgid "parse-value-exception-any-class"
msgstr "\"${value}\" ne peut être analysé en aucune classe du groupe ${classes}"

msgid "no-message-available"
msgstr "Aucun message n'est disponible."

msgid "validations-is-not-list"
msgstr "Les validations doivent être une liste."

msgid "bad-validation-instructions-missing-field"
msgstr "Champ manquant : ${field}"

msgid "bad-validation-instructions-setting-in"
msgstr "Le réglage \"in\" doit être soit \"request\" ou \"response\""

msgid "bad-validation-instructions-bad-schema"
msgstr "Le schéma ${schema} n'est pas valide"

msgid "bad-validation-instructions-schema-not-found"
msgstr "Le schéma requis ${schema} n'a pas pu être trouvé pour effectuer la validation."

