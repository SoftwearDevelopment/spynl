msgid "about-message"
msgstr "Esta es una aplicación web de Spynl. Puede obtener más información en about/endpoints, about/ini, about/versions, about/build y about/environment."

msgid "validation-error"
msgstr "Lo sentimos, ha habido un problema con su solicitud. Si el problema persiste, póngase en contacto con nuestro equipo de atención al cliente. Le pedimos disculpas por las molestias."

msgid "no-endpoint-for-path"
msgstr "No se ha encontrado un punto de conexión para la ruta '${path}'."

msgid "permission-denial"
msgstr "Se ha denegado el permiso para '${permission}' ${context}."

msgid "internal-server-error"
msgstr "Se ha producido un error interno del servidor."

msgid "bad-origin"
msgstr "No se permiten las solicitudes para la API de Spynl desde el origen '${origin}'."

msgid "missing-parameter"
msgstr "Falta el parámetro obligatorio: ${param}"

msgid "illegal-parameter"
msgstr "Parámetro ilegal: ${param}"

msgid "bad-validation-instructions"
msgstr "No se puede usar la descripción de las validaciones para este punto de conexión: ${error}"

msgid "invalid-response"
msgstr "Spynl no ha podido generar una respuesta válida: ${error}"

msgid "email-tmpl-not-found"
msgstr "No se ha encontrado la plantilla <${template}> de correo electrónico."

msgid "email-recipient-not-given"
msgstr "No ha indicado un destinatario para el correo electrónico."

msgid "undetermined-content-type-exception"
msgstr "La solicitud tiene cuerpo pero no se puede determinar el tipo de contenido."

msgid "unsupported-content-type-exception"
msgstr "Tipo de contenido no compatible: \"${type}\""

msgid "deserialization-unsupported-exception"
msgstr "La deserialización para el tipo de contenido \"${type}\" no es compatible."

msgid "serialization-unsupported-exception"
msgstr "La serialización para el tipo de contenido: \"${type}\" no es compatible."

msgid "malformed-request-exception-type"
msgstr "Solicitud \"${type}\" malformada: ${request}"

msgid "malformed-request-exception"
msgstr "Solicitud malformada: ${type}"

msgid "date-decode-value-error"
msgstr "El valor '${value}' para la clave '${key}' no parece que sea una cadena de fecha válida que se ajuste a ${format}."

msgid "unexpected-end-of-input"
msgstr "Fin de entrada inesperada."

msgid "parse-value-exception-as-class"
msgstr "\"${value}\" no se pudo dividir como ${class}"

msgid "parse-value-exception-not-class"
msgstr "${class} no es una clase. No se puede dividir el valor \"${value}\"."

msgid "parse-value-exception-any-class"
msgstr "\"${value}\" no se pudo dividir en ninguna clase en ${classes}"

msgid "no-message-available"
msgstr "No hay ningún mensaje disponible."

msgid "validations-is-not-list"
msgstr "Las validaciones deben ser una lista."

msgid "bad-validation-instructions-missing-field"
msgstr "Campo que falta: ${field}"

msgid "bad-validation-instructions-setting-in"
msgstr "La configuración \"in\" debería ser \"request\" o \"response\""

msgid "bad-validation-instructions-bad-schema"
msgstr "El esquema ${schema} no es válido"

msgid "bad-validation-instructions-schema-not-found"
msgstr "No se ha podido encontrar el esquema requerido ${schema} para realizar la validación."

