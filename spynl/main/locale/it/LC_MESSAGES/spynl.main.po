msgid "about-message"
msgstr "Questa è un'applicazione web Spynl. Puoi trovare ulteriori informazioni su about/endpoints, about/ini, about/versions, about/build e about/environment."

msgid "validation-error"
msgstr "Spiacenti, si è verificato un errore con la tua richiesta. Se il problema persiste, contatta il nostro team di assistenza. Ci scusiamo per l'inconveniente."

msgid "no-endpoint-for-path"
msgstr "Nessun risultato trovato per il percorso '${path}'."

msgid "permission-denial"
msgstr "Il permesso per '${permission}' ${context} è stato negato."

msgid "internal-server-error"
msgstr "Si è verificato un errore all'interno del server."

msgid "bad-origin"
msgstr "Le richieste per l'API Spynl originate da '${origin}' non sono consentite."

msgid "missing-parameter"
msgstr "Parametro richiesto mancante: ${param}"

msgid "illegal-parameter"
msgstr "Parametro illegale: ${param}"

msgid "bad-validation-instructions"
msgstr "La descrizione delle convalide per questo risultato non può essere usata: ${error}"

msgid "invalid-response"
msgstr "Spynl non può generare una risposta valida: ${error}"

msgid "email-tmpl-not-found"
msgstr "Il template per email <${template}> non è stato trovato."

msgid "email-recipient-not-given"
msgstr "Non hai indicato un destinatario per l'email."

msgid "undetermined-content-type-exception"
msgstr "La richiesta ha un corpo, ma il tipo di contenuto non può essere determinato."

msgid "unsupported-content-type-exception"
msgstr "Tipo di contenuto non supportato: \"${type}\""

msgid "deserialization-unsupported-exception"
msgstr "La deserializzazione per i contenuti di tipo \"${type}\" non è supportata."

msgid "serialization-unsupported-exception"
msgstr "La serializzazione per i contenuti di tipo: \"${type}\" non è supportata."

msgid "malformed-request-exception-type"
msgstr "Richiesta: ${request}" malposta \"${type}\"

msgid "malformed-request-exception"
msgstr "Richiesta malposta: ${type}"

msgid "date-decode-value-error"
msgstr "Il valore '${value}' della chiave '${key}' sembra non essere una stringa valida per la data conforme a ${format}."

msgid "unexpected-end-of-input"
msgstr "Interruzione inaspettata degli input."

msgid "parse-value-exception-as-class"
msgstr "\"${value}\" non può essere analizzato come ${class}"

msgid "parse-value-exception-not-class"
msgstr "${class} non è una classe. Impossibile analizzare il valore \"${value}\"."

msgid "parse-value-exception-any-class"
msgstr "\"${value}\" Non può essere analizzato in nessuna classe in ${classes}"

msgid "no-message-available"
msgstr "Nessun messaggio disponibile."

msgid "validations-is-not-list"
msgstr "Le convalide dovrebbero essere elencate."

msgid "bad-validation-instructions-missing-field"
msgstr "Campo mancante: ${field}"

msgid "bad-validation-instructions-setting-in"
msgstr "Le impostazioni \"in\" dovrebbero essere in \"request\" o \"response\""

msgid "bad-validation-instructions-bad-schema"
msgstr "Lo schema ${schema} non è valido"

msgid "bad-validation-instructions-schema-not-found"
msgstr "Impossibile trovare lo schema ${schema} richiesto per effettuare la convalida."

