msgid "about-message"
msgstr "Dies ist eine Spynl Web-Anwendung. Weitere Informationen finden Sie unter über/Endpunkte, über/INI, über/Versionen, über/Struktur und über/Umgebung."

msgid "validation-error"
msgstr "Leider ist bei Ihrer Anfrage ein Problem aufgetreten. Sollte es fortbestehen, setzen Sie sich bitte mit unserem Support-Team in Verbindung. Für die Unannehmlichkeit möchten wir uns entschuldigen."

msgid "no-endpoint-for-path"
msgstr "Für Pfad '${path}‘ wurde kein Endpunkt gefunden."

msgid "permission-denial"
msgstr "Genehmigung für '${permission}' ${context} wurde abgelehnt."

msgid "internal-server-error"
msgstr "Es ist ein interner Serverfehler aufgetreten."

msgid "bad-origin"
msgstr "Eine von '${origin}‘ ausgehende Anfrage an die Spynl-API ist nicht gestattet."

msgid "missing-parameter"
msgstr "Fehlender obligatorischer Parameter: ${param}"

msgid "illegal-parameter"
msgstr "Unzulässiger Parameter: ${param}"

msgid "bad-validation-instructions"
msgstr "Die Validierungsbeschreibung für diesen Endpunkt lässt sich nicht verwenden: ${error}"

msgid "invalid-response"
msgstr "Spynl konnte keine gültige Antwort generieren: ${error}"

msgid "email-tmpl-not-found"
msgstr "Die E-Mail-Vorlage <${template}> wurde nicht gefunden."

msgid "email-recipient-not-given"
msgstr "Sie haben bei dieser E-Mail keinen Empfänger angegeben."

msgid "undetermined-content-type-exception"
msgstr "Die Anfrage enthält einen Hauptteil, doch der Content-Typ kann nicht ermittelt werden."

msgid "unsupported-content-type-exception"
msgstr "Nicht unterstützter Content-Typ: \"${type}\""

msgid "deserialization-unsupported-exception"
msgstr "Deserialisierung für Content-Typ \"${type}\" wird nicht unterstützt."

msgid "serialization-unsupported-exception"
msgstr "Serialisierung für Content-Typ: \"${type}\" wird nicht unterstützt."

msgid "malformed-request-exception-type"
msgstr "Fehlerhafte \"${type}\" Anfrage: ${request}"

msgid "malformed-request-exception"
msgstr "Fehlerhafte Anfrage: ${type}"

msgid "date-decode-value-error"
msgstr "Der Wert '${value}' für den Schlüssel '${key}' ist anscheinend kein gültiger Datumsstring, der ${format} entspricht."

msgid "unexpected-end-of-input"
msgstr "Unerwartetes Eingabeende."

msgid "parse-value-exception-as-class"
msgstr "\"${value}\" konnte nicht als ${class} analysiert werden."

msgid "parse-value-exception-not-class"
msgstr "${class} ist keine Klasse. Die Analyse als \"${value}\\“ ist nicht möglich."

msgid "parse-value-exception-any-class"
msgstr "\"${value}\" konnte in ${classes} nicht als Klasse analysiert werden."

msgid "no-message-available"
msgstr "Keine Nachrichten verfügbar."

msgid "validations-is-not-list"
msgstr "Validierungen müssen als Liste vorliegen."

msgid "bad-validation-instructions-missing-field"
msgstr "Fehlendes Feld: ${field}"

msgid "bad-validation-instructions-setting-in"
msgstr "Die Einstellung „in“ muss entweder „Anfrage“ oder „Antwort“ lauten."

msgid "bad-validation-instructions-bad-schema"
msgstr "Das Schema ${schema} ist nicht gültig."

msgid "bad-validation-instructions-schema-not-found"
msgstr "Das erforderliche Schema ${schema} konnte zur Durchführung der Validierung nicht gefunden werden."

